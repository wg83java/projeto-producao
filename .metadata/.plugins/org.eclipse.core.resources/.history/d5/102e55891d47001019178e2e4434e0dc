import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Builder
public class Pedido extends CBAbstract{
	
	private Long idproduto;
	private String  tipo;
	private String cor;
	private Integer quantidade;

}
